                    $(this).show();
                } else {
                    $(this).hide();
                }

            });

            localStorage.removeItem("pesquisaValue2");

        }


        $('#btVerTodos').click(function () {
            $('#searchTodos').val('')
            $('.live-search-list li').show()

        });

        
