    <script>

        $('#pesquisaHome').click(function () {
            localStorage.setItem("pesquisaValue", $('#pesquisaValueHome').val());
        });

        $('#pesquisaDetalhes').click(function () {
            localStorage.setItem("pesquisaValue2", $('#pesquisaDetalhesValue').val());
        });



        var pesquisaValue = localStorage.getItem("pesquisaValue");
        var pesquisaValue2 = localStorage.getItem("pesquisaValue2");

     

        if (pesquisaValue) {
            $('#searchTodos').val(pesquisaValue)

            $('.live-search-list li').each(function () {
                $(this).attr('data-search-term', $(this).text().toLowerCase());
            });

            var searchTerm = $('#searchTodos').val().toLowerCase();

            $('.live-search-list li').each(function () {

                if ($(this).filter('[data-search-term *= ' + searchTerm + ']').length > 0 || searchTerm.length < 1) {
                    $(this).show();
                } else {
                    $(this).hide();
                }

            });

            localStorage.removeItem("pesquisaValue");

        }

        if (pesquisaValue2) {
            $('#searchTodos').val(pesquisaValue2)

            $('.live-search-list li').each(function () {
                $(this).attr('data-search-term', $(this).text().toLowerCase());
            });

            var searchTerm = $('#searchTodos').val().toLowerCase();

            $('.live-search-list li').each(function () {

